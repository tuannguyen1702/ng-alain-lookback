{
  "name": "backend",
  "version": "0.0.2",
  "description": "This is a project to build API service base o loopback 4.0",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t backend .",
    "docker:run": "docker run -p 3000:3000 -d backend",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "rebuild": "yarn run clean && yarn run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Tuan Nguyen <tuan.nguyen@pops.vn>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "7.3.0",
    "@loopback/authentication-jwt": "0.9.0",
    "@loopback/authorization": "0.9.0",
    "@loopback/boot": "3.4.0",
    "@loopback/context": "3.16.0",
    "@loopback/core": "2.16.0",
    "@loopback/openapi-v3": "5.3.0",
    "@loopback/repository": "3.6.0",
    "@loopback/rest": "9.3.0",
    "@loopback/rest-explorer": "3.3.0",
    "@loopback/security": "0.5.0",
    "@loopback/service-proxy": "3.2.0",
    "@types/jsonwebtoken": "8.5.1",
    "@types/yaml": "1.9.7",
    "bcryptjs": "2.4.3",
    "casbin": "5.7.1",
    "debug": "4.3.1",
    "dotenv": "9.0.2",
    "express": "4.17.1",
    "isemail": "3.2.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "loopback-connector-grpc": "2.0.0",
    "loopback-connector-kv-redis": "4.0.0",
    "loopback-connector-mongodb": "6.0.1",
    "loopback-connector-rest": "4.0.1",
    "nodemailer": "6.6.1",
    "uuid": "8.3.2",
    "yaml": "1.10.2"
  },
  "devDependencies": {
    "@grpc/grpc-js": "1.3.2",
    "@loopback/build": "6.4.0",
    "@loopback/testlab": "3.4.0",
    "@types/bcryptjs": "2.4.2",
    "@types/debug": "4.1.5",
    "@types/express": "4.17.12",
    "@types/lodash": "4.14.170",
    "@types/mocha": "8.2.2",
    "@types/node": "10.17.35",
    "@types/nodemailer": "6.4.1",
    "@types/uuid": "8.3.0",
    "concurrently": "6.2.0",
    "cypress": "7.4.0",
    "mocha": "8.4.0",
    "source-map-support": "0.5.19",
    "typescript": "4.3.2"
  }
}
